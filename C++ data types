#include <bits/stdc++.h>
using namespace std;

// } Driver Code Ends
class Solution {
  public:
    int cppIntType() {
         int a;
       cin>>a;
       return a; // code here
    }
    
    char cppCharType() {
         char c;
       cin>>c;
       return c; // code here
    }
    
    float cppFloatType() {
          float b;
       cin>>b;
       return b;// code here
    }
};

//{ Driver Code Starts.
int main() {
    int t;
    cin >> t;
    while (t--) {
        Solution ob;
        cout << ob.cppIntType() << endl;
        cout << ob.cppCharType() << endl;
        cout << ob.cppFloatType() << endl;
    }
    return 0;
}
